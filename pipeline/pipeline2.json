{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Get Metadata1",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DataSetGetMetadada",
						"type": "DatasetReference",
						"parameters": {
							"SistemaDeArchivos": "fsdiplomado",
							"Directorio": "Virii"
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Get Metadata1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get Metadata1').output.childItems",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Data flow1",
							"type": "ExecuteDataFlow",
							"dependsOn": [],
							"policy": {
								"timeout": "1.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataflow": {
									"referenceName": "Dataflow2SQL",
									"type": "DataFlowReference",
									"parameters": {
										"SistemaArchivos": {
											"value": "'@{pipeline().parameters.SistemasArchivosPipe}'",
											"type": "Expression"
										},
										"DirectorioOrigen": {
											"value": "'Viri/@{item()['name']}'",
											"type": "Expression"
										},
										"NombreArchivo": {
											"value": "'@{item()['name']}*.parquet'",
											"type": "Expression"
										},
										"NombreTabla": {
											"value": "'clean_@{item()['name']}'",
											"type": "Expression"
										},
										"EsquemaDestino": {
											"value": "'@{pipeline().parameters.EsquemaDestinoPipe}'",
											"type": "Expression"
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"SistemasArchivosPipe": {
				"type": "string",
				"defaultValue": "fsdiplomado"
			},
			"EsquemaDestinoPipe": {
				"type": "string",
				"defaultValue": "SalesLT"
			}
		},
		"variables": {
			"Validacion": {
				"type": "String"
			}
		},
		"annotations": []
	}
}